{
  "name": "@cachemap/types",
  "version": "4.0.1",
  "description": "The Cachemap types module.",
  "keywords": [
    "cachemap",
    "cache"
  ],
  "homepage": "https://github.com/badbatch/cachemap",
  "bugs": "https://github.com/badbatch/cachemap/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/badbatch/cachemap"
  },
  "license": "MIT",
  "author": "Dylan Aubrey",
  "main": "lib/main/index.js",
  "module": "lib/module/index.mjs",
  "types": "lib/types/index.d.ts",
  "scripts": {
    "compile": "concurrently npm:compile:*",
    "compile:main": "cross-env BABEL_ENV=main babel ./src --out-dir ./lib/main --extensions \".ts\" --source-maps --config-file ../../babel.config.js",
    "compile:module": "cross-env BABEL_ENV=main babel ./src --out-dir ./lib/main --extensions \".ts\" --out-file-extension \".mjs\" --source-maps --config-file ../../babel.config.js",
    "compile:types": "tsc --build"
  },
  "publishConfig": {
    "access": "public"
  }
}