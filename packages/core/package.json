{
  "name": "@cachemap/core",
  "version": "4.0.4",
  "description": "The Cachemap core module.",
  "author": "Dylan Aubrey",
  "license": "MIT",
  "homepage": "https://github.com/badbatch/cachemap",
  "bugs": "https://github.com/badbatch/cachemap/issues",
  "repository": {
    "directory": "packages/core",
    "type": "git",
    "url": "https://github.com/badbatch/cachemap"
  },
  "type": "module",
  "main": "./dist/main/index.mjs",
  "types": "./dist/types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm run clean:dist && pnpm run compile",
    "clean:deps": "del-cli ./node_modules",
    "clean:dist": "del-cli ./dist",
    "compile": "pnpm run /^compile:.*/",
    "compile:main": "rollup -c ../../rollup.config.cjs",
    "compile:types": "tsc --project ./tsconfig.build.json"
  },
  "dependencies": {
    "@cachemap/constants": "workspace:../constants",
    "@cachemap/controller": "workspace:../controller",
    "@cachemap/map": "workspace:../map",
    "@cachemap/types": "workspace:../types",
    "js-base64": "^3.6.0",
    "cacheability": "^3.0.2",
    "crypto-js": "^4.0.0",
    "eventemitter3": "^4.0.7",
    "md5": "^2.2.1",
    "object-sizeof": "^1.5.3"
  },
  "peerDependencies": {
    "@babel/runtime": "<8",
    "core-js": "<4",
    "lodash": "<5"
  },
  "devDependencies": {
    "@types/md5": "^2.1.33"
  },
  "keywords": [
    "cachemap",
    "cache",
    "core"
  ]
}
