{
  "name": "@cachemap/indexed-db",
  "description": "The Cachemap IndexedDB module.",
  "version": "5.0.0",
  "author": "Dylan Aubrey",
  "license": "MIT",
  "homepage": "https://github.com/badbatch/cachemap",
  "repository": {
    "directory": "packages/indexed-db",
    "type": "git",
    "url": "https://github.com/badbatch/cachemap"
  },
  "bugs": "https://github.com/badbatch/cachemap/issues",
  "type": "module",
  "main": "./dist/main/index.mjs",
  "types": "./dist/types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm run clean:dist && pnpm run compile",
    "clean:deps": "del-cli ./node_modules",
    "clean:dist": "del-cli ./dist",
    "compile": "pnpm run /^compile:.*/",
    "compile:main": "rollup -c ../../rollup.config.cjs",
    "compile:types": "tsc --project ./tsconfig.build.json"
  },
  "dependencies": {
    "@cachemap/types": "workspace:../types",
    "idb": "^5.0.1"
  },
  "peerDependencies": {
    "@babel/runtime": "<8",
    "core-js": "<4",
    "lodash-es": "<5"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.14.191",
    "lodash-es": "^4.17.21"
  },
  "keywords": [
    "IndexedDB",
    "cache",
    "cachemap"
  ]
}
