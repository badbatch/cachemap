{
  "name": "@cachemap/utils",
  "description": "The Cachemap utils module.",
  "version": "5.2.1",
  "author": "Dylan Aubrey",
  "license": "MIT",
  "homepage": "https://github.com/badbatch/cachemap",
  "repository": {
    "directory": "packages/utils",
    "type": "git",
    "url": "https://github.com/badbatch/cachemap"
  },
  "bugs": "https://github.com/badbatch/cachemap/issues",
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/cjs/index.d.cts",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/types/esm/index.d.ts",
        "require": "./dist/types/cjs/index.d.cts"
      },
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm run clean:dist && pnpm run compile",
    "clean:deps": "del-cli ./node_modules",
    "clean:dist": "del-cli ./dist",
    "compile": "pnpm run /^compile:.*/",
    "compile:cjs": "MODULE_SYSTEM=cjs rollup -c ../../rollup.config.cjs",
    "compile:esm": "rollup -c ../../rollup.config.cjs",
    "compile:types": "tsc --project ./tsconfig.build.json && cts-types build dist/types/esm dist/types/cjs"
  },
  "dependencies": {
    "@cachemap/types": "workspace:*",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash-es": "^4.17.12",
    "cacheability": "^4.0.26",
    "core-js": "^3.39.0",
    "crypto-js": "^4.2.0",
    "js-base64": "^3.7.7",
    "lodash-es": "^4.17.21",
    "type-fest": "^4.26.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "cts-types": "^0.0.7",
    "del-cli": "^6.0.0"
  },
  "keywords": [
    "cache",
    "cachemap"
  ]
}
